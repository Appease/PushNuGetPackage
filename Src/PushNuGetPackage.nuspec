<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>PushNuGetPackage</id>
		<version>$version$</version>
		<authors>Chris Dostert</authors>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>
			A PoshCI step that pushes one or more NuGet packages to a .nupkg source
		</description>
		<tags>Powershell PoshCI NuGet Package .nupkg</tags>
		<projectUrl>https://github.com/PoshCI/PushNuGetPackage</projectUrl>
	</metadata>	
	<files>
		<file src="**" target="tools\PushNuGetPackage" exclude="**\.nuspec"/>
	</files>
</package>
